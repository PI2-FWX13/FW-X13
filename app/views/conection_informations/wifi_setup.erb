<h1>Setup Wifi Connection</h1>

  <div class="field">
    <%= form.label :wifi_name %>
    <%= form.text_field :wifi_name, id: :conection_information_wifi_name %>
  </div>

  <div class="field">
    <%= form.label :password %>
    <%= form.text_field :ip, id: :conection_information_password %>
  </div>
  <div class="actions">
    <div style="text-align:center;">
    <%= form.submit 'Connect' , :class => 'waves-effect waves-light btn'%>
    </div>
  </div>
<% end %>
<div id="back-btn">
<%= link_to 'Back', conection_information_index_path , :id => 'back-btn'%>
</div>
