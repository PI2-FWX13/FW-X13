      <h2 class="header">Windings</h2>

          <%= form_tag(windings_path, :method => "get", id: "search-form") do %>
          <%= text_field_tag :search, params[:search], placeholder: "Search Project" %>
          <%= submit_tag "Search" %>
          <% end %>

          <table class="striped">
            <thead>
              <tr>
                <th>Project Name</th>
                <th>Windingdate</th>
                <th colspan="3"></th>
              </tr>
            </thead>

            <tbody>
              <% for winding in @windings %>
                <tr>
                  <td><%= winding.projectName %></td>
                  <td><%= winding.windingdate %></td>
                  <td><%= link_to 'Show', show_winding_path(winding) %></td>
                  <td><%= link_to 'Destroy', destroy_winding_path(winding), method: :delete, data: { confirm: 'Are you sure?' } %></td>
                </tr>
              <% end %>
            </tbody>
          </table>
    <div/>
