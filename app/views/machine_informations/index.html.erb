<p id="notice"><%= notice %></p>

<h1>Machine Informations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Ip</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @machine_informations.each do |machine_information| %>
      <tr>
        <td><%= machine_information.name %></td>
        <td><%= machine_information.ip %></td>
        <td><%= link_to 'Show', index_sessions_path%></td>
        <td><%= link_to 'Edit', edit_machine_information_path(machine_information) %></td>
        <td><%= link_to 'Destroy', machine_information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Machine Information', new_machine_information_path %>
